
<div class="usersContainer">
  <h2>This is the list of users</h2>
  {{#if canSee}}
    <h3>Administration:</h3>
    {{#each users}}
      {{> userList}}
    {{/each}}
    </br>
    <h3>Students:</h3>
  {{/if}}

  {{#each students}}
    {{> userList}}
  {{/each}}

  {{#if BOSS}}
    <a href="/users/add" class="btn btn-secondary btnAdd roundedBtn w100">Add user</a>
  {{/if}}
</div>
